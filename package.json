{
  "name": "portfoli-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "reset": "npx prisma migrate reset",
    "test:silent": "jest --silent",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "tsc": "tsc --watch",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "migrate:docker": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init",
    "seed:docker": "dotenv -e .env.test -- npx prisma db seed --preview-feature",
    "reset:docker": "dotenv -e .env.test -- npx prisma migrate reset",
    "test": "dotenv -e .env.test npm run test:silent",
    "do:you:remember": "psql -U prisma -W tests && dt command and select all from routes"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.7",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@prisma/client": "^4.6.1",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "framer-motion": "^6.5.1",
    "next": "13.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0",
    "swiper": "^8.4.4",
    "typewriter-effect": "^2.19.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-next": "^13.0.3",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "prisma": "^4.6.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
